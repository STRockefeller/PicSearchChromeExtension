{
    "name": "More image - More idea - More inspiration",
    "description": "An image search helper. ",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": ["contextMenus", "storage"],
    "action": {
        "default_icon": "icons/Icon-192.png",
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html"
    },
    "background": {
        "service_worker": "event.js"
    }
}